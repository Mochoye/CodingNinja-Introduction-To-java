import java.util.*;
public class Solution {
    
    public static void main(String[] args) {
        // Write your code here
        Scanner sc=new Scanner(System.in);
        int x=sc.nextInt();
        int n=sc.nextInt();
        int res=1;
        for(int i =0;i<n;i++)
        {
            res*=x;
        }
        System.out.println(res);
    }
}
