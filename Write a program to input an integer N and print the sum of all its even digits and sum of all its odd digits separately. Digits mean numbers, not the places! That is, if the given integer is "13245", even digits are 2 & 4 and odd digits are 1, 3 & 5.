import java.util.*;
public class Main {
	
	public static void main(String[] args) {
        
		// Write your code here
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int os=0,es=0;
        while(n!=0)
        {
            if((n%10)%2==0)
                es+=(n%10);
            else
                os+=(n%10);
            n/=10;
        }
        System.out.println(es+" "+os);

	}
}
